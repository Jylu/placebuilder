{
  "info_box": <%= html_to_json {render 'info_box', :user => @user} %>,
  "map": {
    "directions": {
      "from": {
        "lat": <%= current_user.lat %>,
        "lng": <%= current_user.lng %>
      },
      "to": {
        "lat": <%= @user.lat %>,
        "lng": <%= @user.lng %>
      }
    }
  }

}