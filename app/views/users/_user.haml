- content_tag(:li, :class => "person", "data-info" => render("users/user_info_box", :user => user), "data-marker" => user.lat && "{\"info_html\": \"#{user.full_name}\", \"lat\": \"#{user.lat}\", \"lng\": \"#{user.lng}\"}") do
  = image_tag user.avatar.url, :width => 60, :height => 60
  %p #{user.full_name}