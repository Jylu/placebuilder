<script  type="text/javascript">
var ids=new Array();
var currentid=1;
function show(id){
//var select=$(".multiselect").clone(false);
if(ids.indexOf(id)>=0){
  return;
}
else{
  ids.push(id);
  var newid=currentid++;
  var newselect=$("<select id="+newid+" name=\"filter-tags\" style=\"width:130px\" onchange=\"show()\"><option></option> {{#dropdowns}}<option value=\"{{tag}}\">{{val}}{{/dropdowns}}</select><br>");
newselect.insertBefore("#filter");
console.log(newid);
}

}
</script>
<div id="file-picker-filters">
<button id="new-resident">New Resident</button>
<button id="todo-list">TODO List</button>
<br>
  <input type="text" placeholder="search" id="query-input" size=23>
  <button id="filter-button">Search</button>

<select id="haveornot" name="haveornot" >
    <option value="yes" selected=true> Have
    <option value="no"> Not
  </select>
<select id="1" name="filter-tags" style="width:130px" onchange="show(this.id)">
  <option></option>
  {{#dropdowns}}
        <option value="{{tag}}" > {{val}}
  {{/dropdowns}}
</select>
<button id="filter">Filter</button><br>

<select name="add-tag" style="width:198px">
  <option></option>
   {{#tags}}
     <option class="tag-adding">{{.}}</option>
  {{/tags}}
</select>
<button id="add-tag">Add tag</button>  
<br>
  <button id="filter-order">Order by</button>
  <select id="order-tags" name="order-tags" style="width:130px">
  <option></option>
  {{#dropdowns}}
        <option value="{{tag}}" > {{val}}
  {{/dropdowns}}
</select>
<select id="order">
  <option value=time>Time</option>
  <option value=quantity>Quantity</option>
</select>
<br>
<input type="checkbox" id="check-all">Check all shown</input>
</div>

<ul id="file-picker-list">
<li>Loading...</li>
</ul>
